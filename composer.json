{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-gd": "*",
        "ext-soap": "*",
        "barryvdh/laravel-dompdf": "^0.9",
        "fideloper/proxy": "^4.2",
        "flynsarmy/csv-seeder": "1.*",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "^1.9",
        "inertiajs/inertia-laravel": "^0.4.4",
        "infobip/infobip-api-php-client": "dev-master",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/legacy-factories": "^1.1",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.3",
        "laravelcollective/html": "^6.0",
        "lcobucci/jwt": "^3.4|^4.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^6.2",
        "orangehill/iseed": "^3.0",
        "phpmailer/phpmailer": "^6.1",
        "protonemedia/laravel-cross-eloquent-search": "^2.0",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-query-builder": "^3.3",
        "spatie/laravel-searchable": "^1.9",
        "tightenco/ziggy": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "chmod -R 777 storage",
            "php artisan passport:keys"
        ]
    }
}
